RewriteEngine On    # Turn on the rewriting engine

RewriteRule    ^login/?$    index.php    [NC,L]   
RewriteRule    ^Register/?$    Register.php    [NC,L]   
RewriteRule    ^signout/?$    signout.php    [NC,L]   

RewriteRule    ^ForgotPassword/?$    forgotpass.php    [NC,L]   
RewriteRule    ^ResetPassword/([A-Za-z0-9-]+)/?$    ResetPassword.php?code=$1    [NC,L]   

RewriteRule    ^checkpoint/?$    ip.php    [NC,L]   
RewriteRule    ^Dashboard/?$    home.php    [NC,L]   
RewriteRule    ^ChangePassword/?$    changepass.php    [NC,L]   
RewriteRule    ^Profile/?$    Profile.php    [NC,L]   

RewriteRule    ^Activity/?$    Activity.php    [NC,L]   
RewriteRule    ^Activity/([A-Za-z0-9-]+)/?$    Activity.php?page=$1    [NC,L]   
RewriteRule    ^SendMoney/?$    SendMoney.php    [NC,L]   
RewriteRule    ^Preview/?$    Preview.php    [NC,L]   
RewriteRule    ^Sent/?$    Sent.php    [NC,L]   
RewriteRule    ^RequestMoney/?$    RequestMoney.php    [NC,L]
RewriteRule    ^KYC/?$    Kyc.php    [NC,L]
RewriteRule    ^RequestToMe/?$    RequestToMe.php    [NC,L]   
