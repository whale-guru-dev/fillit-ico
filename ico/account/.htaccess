RewriteRule    ^Refund/([A-Za-z0-9-]+)/?$    Refund.php?id=$1    [NC,L] 

RewriteRule    ^AddMoney/?$    addmoney.php    [NC,L]   
RewriteRule    ^DepositPreview/?$    addmoneyPreview.php    [NC,L]   
RewriteRule    ^DepositNow/?$    addNow.php    [NC,L]   
RewriteRule    ^AddMoneyLog/?$    AddMoneyLog.php    [NC,L]   
RewriteRule    ^AddMoneyLog/([A-Za-z0-9-]+)/?$    AddMoneyLog.php?page=$1    [NC,L]  

RewriteRule    ^WithdrawMoney/?$    withrawmoney.php    [NC,L]   
RewriteRule    ^WithdrawPreview/?$    WithdrawPreview.php    [NC,L]   
RewriteRule    ^WithdrawFinal/?$    WithdrawFinal.php    [NC,L]   
RewriteRule    ^WithdrawLog/?$    WithdrawLog.php    [NC,L]   
RewriteRule    ^WithdrawLog/([A-Za-z0-9-]+)/?$    WithdrawLog.php?page=$1    [NC,L]   
RewriteRule    ^Referral/?$    Referral.php    [NC,L]
RewriteRule    ^API/?$    API.php    [NC,L]   

RewriteRule    ^VerifyMobile/?$    VerifyMobile.php    [NC,L]   
RewriteRule    ^VerifyEmail/?$    VerifyEmail.php    [NC,L]

RewriteRule    ^ExchangeRates/?$    ExchangeRates.php    [NC,L]

RewriteRule    ^cashlibapi/?$    cashlib.php    [NC,L]